    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.paragonsoftware.commons</groupId>
                <artifactId>abstractions</artifactId>
                <version>${paragon.commons.version}</version>
            </dependency>

            <dependency>
