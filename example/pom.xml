	<version>1.0-SNAPSHOT</version>

    <packaging>pom</packaging>


    <modules>
        <module>dto</module>
        <module>service</module>
        <module>service-impl</module>
        <module>rest</module>
        <module>service-via-rest</module>
        <module>test-client</module> 
